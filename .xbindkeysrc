"~/bin/e"
  release+control+alt + e

"run-terminal"
  release+control+alt + t

"toggle-vertical"
  release+control+alt + r

"xdg-screensaver lock"
  release+control+alt + l

"firefox"
  release+control+alt + f

"mpc next"
  release+XF86Forward

"mpc prev"
  release+XF86Back

"mpc toggle"
  release+XF86AudioPlay
